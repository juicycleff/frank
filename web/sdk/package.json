{
  "name": "frank-sdk",
  "version": "1.0.1",
  "description": "Frank Authentication TypeScript Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vanilla": {
      "import": "./dist/vanilla/index.js",
      "types": "./dist/vanilla/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "openapi-ts"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.3",
    "@tanstack/react-query": "~5.69.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.64.13",
    "@types/node": "^20.14.8",
    "@types/react": "^18.2.21",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
