{
  "name": "frank-client",
  "version": "1.0.1",
  "description": "Frank Authentication TypeScript Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vanilla": {
      "import": "./dist/vanilla/index.js",
      "types": "./dist/vanilla/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./model": {
      "import": "./dist/model/index.js",
      "types": "./dist/model/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "orval"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "@tanstack/react-query": "~5.69.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/react": "^18.2.21",
    "orval": "^7.7.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
