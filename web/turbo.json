{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"lint": {
			"outputs": []
		},
		"format": {
			"outputs": []
		},
		"generate": {
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
